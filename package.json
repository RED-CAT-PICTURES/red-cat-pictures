    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/mdc": "^0.19.2",
    "@nuxtjs/seo": "^3.3.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@shba007/unai": "^0.3.7",
    "@tresjs/cientos": "^5.2.0",
    "@tresjs/nuxt": "^5.1.2",
    "@types/k6": "^1.4.0",
    "@types/markdown-it": "^14.1.2",
    "@types/web-push": "^3.6.4",
    "@unhead/vue": "^2.1.1",
    "@vite-pwa/nuxt": "^1.1.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "date-fns": "^4.1.0",
