    "rehype-parse": "^9.0.1",
    "rehype-remark": "^10.0.1",
    "remark-stringify": "^11.0.0",
    "three": "^0.182.0",
    "unified": "^11.0.5",
    "vue": "latest",
    "vue-router": "latest",
    "web-push": "^3.6.7",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@tauri-apps/cli": "^2.9.6",
    "@vue/test-utils": "^2.4.6",
